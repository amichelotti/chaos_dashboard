{
    "title":"Dashboard Settings",
    "type":"object",
	"id":"dashboard-settings",
	"format":"tab",
    "properties": {
	
	"generalRefresh" : {
		"type":"integer",
		"default":1000,
		"description":"General Refresh (ms)"
	},
	"maxNodesPerPage" : {
		"type":"integer",
		"default":10,
		"description":"Max number of Nodes per page"
	},
	"defaultZone" : {
		"type":"string",
		"default":"ALL",
		"description":"Default Zone to open"
	},
	"defaultGroup" : {
		"type":"string",
		"default":"ALL",
		"description":"Default Group to open"
	},
	"generalControlRefresh" : {
		"type":"integer",
		"default":500,
		"description":"Default General Refresh when in control (ms)"
	},
	"defaultPage" : {
		"type":"integer",
		"default":1000,
		"description":"Default History Page"
	},
	"checkLive" : {
		"type":"integer",
		"default":10000,
		"description":"Live Check Refresh (ms)"
	},
	"defaultRestTimeout" : {
		"type":"integer",
		"default":5000,
		"description":"Default Timeout Rest operation (ms)"
	},
	"camera":{
		    "$ref":"camera-settings.json",
		    "title":"Camera Settings"
		
	}
}
}
    
